{
	"name": "Cat's Purple",
	"type": "dark",
	"editor.fontFamily": "GohuFont uni14 Nerd Font Mono",
	"terminal.integrated.fontFamily": "'GohuFont NF'",
	"colors": {
		"focusBorder": "#ff1e74",
		"editor.background": "#1c1c1c",
		"editor.foreground": "#ffffff", //f8eeff
		"activityBarBadge.background": "#6f00ff",
		"sideBarTitle.foreground": "#bbbbbb",
		"button.background": "#6f00ff",
		"icon.foreground": "#6f00ff",
		"activityBar.background": "#1c1c1c",
		"activityBar.activeBackground": "#6f00ff",
		"activityBar.inactiveForeground": "#6f00ff",
		"welcomePage.progress.foreground": "#6f00ff",
		"textLink.foreground": "#9340ff",

		"statusBar.background" : "#1A1A1A",
		"statusBar.noFolderBackground" : "#212121",
		"statusBar.debuggingBackground": "#263238"
	},
	"tokenColors": [
		{
			"name": "Json Key",
			// "key": value
			"scope": [
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#6f00ff"
			}
		},
		{
			"name": "Json Value",
			// key: "value"
			"scope": [
				"string.quoted.double.json"
			],
			"settings": {
				"foreground": "#ffd700"
			}
		},
		{
			"name": "Comment",
			// this
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#5d5d5d"
			}
		},
		{
			"name": "Storage Type",
			// Storage Type
			"scope":[
				"storage.type"
			],
			"settings": {
				"foreground": "#ff3b97"
			}
		},
		{
			"name": "Variable",
			// Variable
			"scope":[
				"variable"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Field",
			// Field
			"scope":[
				"entity.name.variable"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Property",
			// Property
			"scope":[
				"variable.other.object.property",
				"meta.property-name"
			],
			"settings": {
				"foreground": "#b06fff"
			}
		},
		{
			"name": "Keyword",
			// void, if, = yield return new && ==
			"scope":[
				"keyword"
			],
			"settings": {
				"foreground": "#ff3b97"
			}
		},
		{
			"name": "String",
			// "imavariable"
			"scope":[
				"string"
			],
			"settings": {
				"foreground": "#ffd700"
			}
		},
		{
			"name": "Punctuation",
			// (){}[]
			"scope":[
				"punctuation"
			],
			"settings": {
				"foreground": "#00c7ff"
			}
		},
		{
			"name": "Function",
			// Function
			"scope":[
				"entity.name.function"
			],
			"settings": {
				"foreground": "#6f00ff",
				// "fontStyle": "bold"
			}
		},
		{
			"name" : "String punctuation",
			// " ' `
			"scope":[
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#ffd700"
			}
		},
		{
			"name": "Constant",
			// true, false, null
			"scope":[
				"constant.language"
			],
			"settings": {
				"foreground": "#00c7ff"
			}
		},
		{
			"name": "Storage Modifier",
			// public, private, protected
			"scope":[
				"storage.modifier"
			],
			"settings": {
				"foreground": "#b06fff"
			}
		},
		{
			"name": "Type",
			// UnityEvent, MonoBehaviour, GameObject
			"scope":[
				"entity.name.type"
			],
			"settings": {
				"foreground": "#6f00ff"
			}
		},
		{
			"name": "HTML TAG",
			// html tag
			"scope":[
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#6f00ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Attribute name",
			// Attribute name
			"scope":[
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#ff3b97"
			}
		},
	]
}
